 INCLUDE PROLOG.H
_CC1:
 PUBLIC _loop
_loop:
 SUB SP,4
 MOV BX,2
 ADD BX,SP
 XCHG DX,BX
 MOV BX,10
 MOV SI,DX
 MOV [SI],BX
 MOV BX,2
 ADD BX,SP
 MOV BX,[BX]
 XCHG DX,BX
 MOV BX,20
 CMP DX,BX
 MOV BX,CX
 JGE $+3
 INC BX
 OR BX,BX
 JNZ $+5
 JMP _CC3
 MOV BX,0
 ADD BX,SP
 XCHG DX,BX
 MOV BX,5
 MOV SI,DX
 MOV [SI],BX
 JMP _CC4
_CC3:
 MOV BX,0
 ADD BX,SP
 XCHG DX,BX
 MOV BX,6
 MOV SI,DX
 MOV [SI],BX
_CC4:
 MOV BX,0
 ADD BX,SP
 MOV BX,[BX]
 ADD SP,4
 RET
 INCLUDE EPILOG.H
 END
